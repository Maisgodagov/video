{
  "name": "video-processing-pipeline",
  "version": "1.0.0",
  "description": "Pipeline for processing English learning videos with AI",
  "main": "video-pipeline/src/index.js",
  "scripts": {
    "start": "node video-pipeline/src/index.js",
    "process": "node video-pipeline/src/index.js",
    "core": "node video-pipeline/src/index.js --mode=no-exercises",
    "transcription": "node video-pipeline/src/index.js --mode=transcription-only",
